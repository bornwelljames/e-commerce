        <div class="row justify-content-center">
            <div class="col-8 mb-4" id="uploadForm">
                <form action="/products/" class="row g-3" method="POST" accept-charset="UTF-8">
                    <div class="col-md-6">
                      <label for="item-name" class="form-label">Item Name:</label>
                      <input type="text" name="item" class="form-control" id="item-name" placeholder="Name">
                    </div>
                    <div class="col-md-6">
                      <label for="item-category" class="form-label">Item Category:</label>
